################################################Scrollbar
style "scrollbar"
{
	engine "pixmap" 
	{
		image	#horizontal scrollbar base
		{
			function		= BOX
			recolorable		= TRUE
			detail			= "trough"
			file			= "colored/scrollbars/horizontal-background.png"
			border			= { 12,12, 0,0 }
			stretch			= TRUE
			orientation		= HORIZONTAL
		}
		image	#horizontal scrollbar slider normal
		{
			function		= SLIDER
			recolorable		= TRUE
			state		 	= NORMAL
			file			= "colored/scrollbars/horizontal-normal.png"
			border			= {12,12, 0,0 }
			stretch			= TRUE
			orientation		= HORIZONTAL
		}
		image 	#horizontal scrollbar slider active
		{
			function		= SLIDER
			recolorable		= TRUE
			state			= ACTIVE
			shadow			= IN
			file			= "colored/scrollbars/horizontal-normal.png" 
			border			= { 6, 6, 6, 6 }
			stretch			= TRUE
			orientation		= HORIZONTAL
		}
		image 	#horizontal scrollbar slider prelight
		{
			function		= SLIDER
			recolorable		= TRUE
			state			= PRELIGHT
			file			= "colored/scrollbars/horizontal-hover.png" 
			border			= { 6, 6, 6, 6 }
			stretch			= TRUE
			orientation		= HORIZONTAL
		}
		image 	#horizontal scrollbar slider insensitive
		{
			function		= SLIDER
			recolorable		= TRUE
			state			= INSENSITIVE
			file			= "colored/scrollbars/trans.png"
			border			= { 6, 6, 6, 6 }
			stretch			= TRUE
			orientation		= HORIZONTAL

		}
		image	#vertical scrollbar base 
		{
			function		= BOX
			recolorable		= TRUE
			detail			= "trough"
			file			= "colored/scrollbars/vertical-background.png"
			border			= { 0,0, 12,12}
			stretch			= TRUE
			orientation		= VERTICAL
		}
		image 	#vertical scrollbar slider normal
		{
			function		= SLIDER
			recolorable		= TRUE
			state		 	= NORMAL
			file			= "colored/scrollbars/vertical-normal.png"
			border			= { 0,0, 12,12 }
			stretch			= TRUE
			orientation		= VERTICAL
		}
		image	#vertical scrollbar slider active
		{
			function		= SLIDER
			recolorable		= TRUE
			state			= ACTIVE
			shadow			= IN
			file			= "colored/scrollbars/vertical-normal.png" 
			border			= { 6, 6, 6, 6}
			stretch			= TRUE
			orientation		= VERTICAL

		}
		image	#vertical scrollbar slider prelight
		{
			function		= SLIDER
			recolorable		= TRUE
			state			= PRELIGHT
			file			= "colored/scrollbars/vertical-hover.png" 
			border			= { 6, 6, 6, 6}
			stretch			= TRUE
			orientation		= VERTICAL
		}
		image	#vertical scrollbar slider insensitive
		{
			function		= SLIDER
			recolorable		= TRUE
			state			= INSENSITIVE
			file			= "colored/scrollbars/trans.png"
			border			= { 6, 6, 6, 6}
			stretch			= TRUE
			orientation		= VERTICAL
		}
		image	#stepper 
		{
			function		= STEPPER
			recolorable		= TRUE
			file			= "colored/scrollbars/trans.png"
			stretch			= TRUE
		}


	}
}
